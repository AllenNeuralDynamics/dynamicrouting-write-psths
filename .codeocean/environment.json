{
	"version": 1,
	"base_image": "codeocean/mambaforge3:23.1.0-4-python3.10.12-ubuntu22.04",
	"post_install": true,
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true,
		"mount_secrets": true
	},
	"installers": {
		"apt": {
			"packages": [
				{
					"name": "build-essential",
					"version": "12.9ubuntu3"
				}
			],
			"install_software_properties_common": true
		},
		"pip3": {
			"packages": [
				{
					"name": "cloudpickle",
					"version": "3.1.2"
				},
				{
					"name": "lazynwb",
					"version": "0.2.76"
				},
				{
					"name": "numba",
					"version": "0.60.0"
				},
				{
					"name": "polars-ds",
					"version": "0.10.3"
				},
				{
					"name": "pydantic-settings",
					"version": "2.10.1"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}